<Window x:Class="Monopoly.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ui="clr-namespace:/styles/MainWindow.css"

        xmlns:local="clr-namespace:Monopoly"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="root">
        <Grid x:Name="Card1" Background="#FF88D19C" Margin="360,197,352,186">
            <Label x:Name="player1" Content="Label" HorizontalAlignment="Left" Margin="3,2,0,0" VerticalAlignment="Top" Height="10" Width="10" Background="#FF1CB51A"/>
            <Label x:Name="player2" Content="Label" HorizontalAlignment="Left" Margin="17,2,0,0" VerticalAlignment="Top" Height="10" Width="10" Background="#FF1CB51A"/>
            <Label x:Name="player3" Content="Label" HorizontalAlignment="Left" Margin="31,2,0,0" VerticalAlignment="Top" Height="10" Width="10" Background="#FF1CB51A"/>
            <Label x:Name="title" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Center" Height="24" Width="70"/>
        </Grid>
    </Grid>
</Window>
